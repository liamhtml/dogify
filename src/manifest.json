{

  "manifest_version": 2,

  "name": "Dogify",

  "description": "Help Dogify the web.",

  "version": "1.0",

   "background": {
      "scripts": ["popup.js"],
      "persistent": false
    },

   "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "img/dogify16.png",
        "32": "img/dogify32.png",
        "48": "img/dogify48.png",
        "128": "img/dogify128.png"
      }
    }

}
